; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GodotInSim"
run/main_scene="res://demo.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GISInSim="*res://addons/godot_insim/src/insim/insim.gd"
GISOutSim="*res://addons/godot_insim/src/outsim/outsim.gd"
GISOutGauge="*res://addons/godot_insim/src/outgauge/outgauge.gd"

[debug]

gdscript/warnings/exclude_addons=false
gdscript/warnings/untyped_declaration=1
gdscript/warnings/return_value_discarded=1

[display]

window/size/viewport_width=1280
window/size/viewport_height=960

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_insim/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
